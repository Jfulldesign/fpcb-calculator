{
  "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/proposal-optional-chaining",
      "@babel/syntax-dynamic-import",
      [
          "react-css-modules",
          {
              "webpackHotModuleReloading": true
          }
      ]
  ],
  "presets": [
    ["@babel/env", {
        "targets": {
            "ie": "11"
        }
    }],
    "@babel/flow",
    "@babel/react"
  ]
}